<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="607.0" prefWidth="514.0" stylesheets="@../../../../map.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.makery.address.view.SelectLocationController">
    <children>
        <VBox alignment="CENTER" spacing="10.0" AnchorPane.topAnchor="0.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
            <children>
                <Label styleClass="map-label" text="Select Location" />
                <Button fx:id="selectLocationButton" onAction="#handleSelectPortArthur" styleClass="map-button" text="Port Arthur" />
                <Button onAction="#handleSelectTaiPan" styleClass="map-button" text="Tai-Pan" />
                <Button onAction="#handleSelectEdamame" styleClass="map-button" text="Edamame" />
                <Button onAction="#handleSelectLamaSut" styleClass="map-button" text="Lama-Sut" />
                <Button onAction="#handleSelectKingston" styleClass="map-button" text="Kingston" />
                <Button fx:id="backButton" onAction="#handleBack" styleClass="map-button" text="Back" />
            </children>
        </VBox>
    </children>
</AnchorPane>